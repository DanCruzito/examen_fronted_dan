orm-group m-3\">\r\n                <label for=\"autor_id\">Autor</label><br />\r\n                <select class=\"form-control\" v-model=\"libro.autor_id\" id=\"autor_id\">\r\n                    <option value=\"\" selected disabled>Seleccione una opción</option>\r\n                  <option v-for=\"row in autores\" :key=\"row.id\" value=\"\">\r\n                    {{ row.name }}\r\n                  </option>\r\n                </select>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <br />\r\n          <div>\r\n            <button type=\"submit\" class=\"btn btn-success\">Registrar</button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"card\">\r\n      <div class=\"card-body\">\r\n        <table class=\"table table-striped\">\r\n          <thead>\r\n            <tr>\r\n              <th scope=\"col\">#</th>\r\n              <th scope=\"col\">Titulo</th>\r\n              <th scope=\"col\">Lote</th>\r\n              <th scope=\"col\">Acciones</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"(row, index) in libros\" :key=\"row.id\">\r\n              <th scope=\"row\">{{ index + 1 }}</th>\r\n              <td>\r\n                {{ row.titulo }}\r\n              </td>\r\n              <td>\r\n                {{ row.lote }}\r\n              </td>\r\n              <td>\r\n                <router-link\r\n                  :to=\"{ name: 'autor_detalle', params: { id: row.id } }\"\r\n                  ><button class=\"btn btn-warning btn-sm mx-1\">\r\n                    Ver autor\r\n                  </button></router-link\r\n                >\r\n                <router-link\r\n                  :to=\"{ name: 'autor_edit', params: { id: row.id } }\"\r\n                >\r\n                  <button class=\"btn btn-primary btn-sm mx-1\">Editar</button>\r\n                </router-link>\r\n                <button\r\n                  @click=\"deleteAutor(row.id)\"\r\n                  class=\"btn btn-danger btn-sm\"\r\n                >\r\n                  Eliminar\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      juj\r\n      {{ page }}\r\n      <div class=\"justify-content-center\">\r\n        <ul class=\"pagination justify-content-center\">\r\n    <li v-for=\"pag_link in pagination_link\" :key=\"pag_link.label\" \r\n    :class=\"{\r\n            'disabled': pag_link.url == null,\r\n            'active': pag_link.link \r\n        }\"\r\n    class=\"page-item\">\r\n        <a class=\"page-link\"\r\n        href=\"#\" v-html=\"pag_link.label\"></a></li>\r\n    <!--<li class=\"page-item\"><a class=\"page-link\" href=\"#\">1</a></li>\r\n    <li class=\"page-item\"><a class=\"page-link\" href=\"#\">2</a></li>\r\n    <li class=\"page-item\"><a class=\"page-link\" href=\"#\">3</a></li>\r\n    <li class=\"page-item\"><a class=\"page-link\" href=\"#\">Next</a></li>\r\n    -->\r\n  </ul>\r\n      </div>\r\n    </div>\r\n    <nav aria-label=\"Page navigation example\">\r\n  \r\n</nav>\r\n  </div>\r\n</template>\r\n  \r\n  <script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      libros: [],\r\n      autores: [],\r\n      libro: {\r\n        titulo: \"\",\r\n        lote: \"\",\r\n        descripcion: \"\",\r\n        autor_id: \"\",\r\n      },\r\n      errors: {},\r\n      pagination_link: []\r\n    };\r\n  },\r\n  created() {\r\n    this.getLibros();\r\n    this.getAutores();\r\n  },\r\n  computed:{\r\n   page(){\r\n    return this.$route.query.page ?? 1;\r\n   }\r\n  },\r\n  methods: {\r\n    getLibros() {\r\n      this.axios\r\n        .get(\"http://127.0.0.1:8000/api/libro?per_page=10\")\r\n        .then((response) => {\r\n          console.log(response.data.data);\r\n          let res = response.data;\r\n          this.libros = res.data;\r\n          this.pagination_link = res.links;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    getAutores() {\r\n      this.axios\r\n        .get(\"http://127.0.0.1:8000/api/autor\")\r\n        .then((response) => {\r\n          console.log(response.data);\r\n          this.autores = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    saveLibro() {\r\n      //alert('jola')\r\n      this.axios\r\n        .post(\"http://127.0.0.1:8000/api/autor-libro\", this.libro)\r\n        .then((response) => {\r\n          console.log(response.data);\r\n\r\n          this.autores.push(response.data);\r\n          this.autor = {\r\n            name: \"\",\r\n          };\r\n\r\n          this.errors = [];\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n          this.errors = Object.values(error.response.data.errors).flat();\r\n        });\r\n    },\r\n    deleteAutor(id) {\r\n      alert(id);\r\n      this.axios\r\n        .get(\"http://127.0.0.1:8000/api/autor-eliminar/\" + id)\r\n        .then(() => {\r\n          this.autores = this.autores.filter((autor) => autor.id != id);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n  \r\n  <style>\r\n</style>"],"mappings":";AAuIA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE;QACLC,MAAM,EAAE,EAAE;QACVC,IAAI,EAAE,EAAE;QACRC,WAAW,EAAE,EAAE;QACfC,QAAQ,EAAE;MACZ,CAAC;MACDC,MAAM,EAAE,CAAC,CAAC;MACVC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC,CAAC;IAChB,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,QAAQ,EAAC;IACRC,IAAIA,CAAA,EAAE;MACL,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,IAAG,IAAK,CAAC;IACnC;EACD,CAAC;EACDG,OAAO,EAAE;IACPN,SAASA,CAAA,EAAG;MACV,IAAI,CAACO,KAAI,CACNC,GAAG,CAAC,6CAA6C,EACjDC,IAAI,CAAEC,QAAQ,IAAK;QAClBC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACrB,IAAI,CAACA,IAAI,CAAC;QAC/B,IAAIwB,GAAE,G