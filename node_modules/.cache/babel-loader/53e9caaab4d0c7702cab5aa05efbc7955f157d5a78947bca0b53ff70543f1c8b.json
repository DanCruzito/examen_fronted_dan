{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      libros: [],\n      autores: [],\n      libro: {\n        titulo: \"\",\n        lote: \"\",\n        descripcion: \"\",\n        autor_id: \"\"\n      },\n      errors: {}\n    };\n  },\n  created() {\n    this.getLibros();\n    this.getAutores();\n  },\n  methods: {\n    getLibros() {\n      this.axios.get(\"http://127.0.0.1:8000/api/libro\").then(response => {\n        console.log(response.data);\n        this.libros = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    getAutores() {\n      this.axios.get(\"http://127.0.0.1:8000/api/autor\").then(response => {\n        console.log(response.data);\n        this.autores = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    saveLibro() {\n      //alert('jola')\n      this.axios.post(\"http://127.0.0.1:8000/api/autor-libro\", this.libro).then(response => {\n        console.log(response.data);\n        this.autores.push(response.data);\n        this.autor = {\n          name: \"\"\n        };\n        this.errors = [];\n      }).catch(error => {\n        console.log(error);\n        this.errors = Object.values(error.response.data.errors).flat();\n      });\n    },\n    deleteAutor(id) {\n      alert(id);\n      this.axios.get(\"http://127.0.0.1:8000/api/autor-eliminar/\" + id).then(() => {\n        this.autores = this.autores.filter(autor => autor.id != id);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","libros","autores","libro","titulo","lote","descripcion","autor_id","errors","created","getLibros","getAutores","methods","axios","get","then","response","console","log","catch","error","saveLibro","post","push","autor","name","Object","values","flat","deleteAutor","id","alert","filter"],"sources":["C:\\www\\vue_fronted\\src\\views\\libro\\libro_lista.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h1>Listado de Libros</h1>\r\n    <div class=\"card m-2\">\r\n      <div class=\"card body\">\r\n        <ul v-if=\"errors.length > 0\">\r\n          <li class=\"text-danger\" v-for=\"error in errors\" :key=\"error.id\">\r\n            {{ error }}\r\n          </li>\r\n        </ul>\r\n        <form @submit.prevent=\"saveLibro\">\r\n          <div class=\"row\">\r\n            <div class=\"col-md-6\">\r\n              <div class=\"form-group m-3\">\r\n                <label for=\"titulo\">Titulo</label>\r\n                <input\r\n                  class=\"form-control\"\r\n                  v-model=\"libro.titulo\"\r\n                  type=\"text\"\r\n                  id=\"titulo\"\r\n                />\r\n              </div>\r\n            </div>\r\n            <div class=\"col-md-\">\r\n                <div class=\"form-group m-3\">\r\n            <label for=\"lote\">Lote</label><br />\r\n            <input\r\n              class=\"form-control\"\r\n              v-model=\"libro.lote\"\r\n              type=\"text\"\r\n              id=\"lote\"\r\n            />\r\n          </div>\r\n            </div>\r\n          </div>\r\n\r\n          \r\n          <div class=\"form-group m-3\">\r\n            <label for=\"descripcion\">Descripción</label><br />\r\n            <input\r\n              class=\"form-control\"\r\n              v-model=\"libro.descripcion\"\r\n              type=\"text\"\r\n              id=\"descripcion\"\r\n            />\r\n          </div>\r\n          <div class=\"form-group m-3\">\r\n            <label for=\"autor_id\">Descripción</label><br />\r\n            <select v-model=\"libro.autor_id\" id=\"autor_id\">\r\n              <option v-for=\"row in autores\" :key=\"row.id\" value=\"\">\r\n                {{ row.name }}\r\n              </option>\r\n            </select>\r\n            <input\r\n              class=\"form-control\"\r\n              v-model=\"libro.autor_id\"\r\n              type=\"text\"\r\n              id=\"autor_id\"\r\n            />\r\n          </div>\r\n          <br />\r\n          <div>\r\n            <button type=\"submit\" class=\"btn btn-success\">Registrar</button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"card\">\r\n      <div class=\"card-body\">\r\n        <table class=\"table table-striped\">\r\n          <thead>\r\n            <tr>\r\n              <th scope=\"col\">#</th>\r\n              <th scope=\"col\">Titulo</th>\r\n              <th scope=\"col\">Lote</th>\r\n              <th scope=\"col\">Acciones</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"(row, index) in libros\" :key=\"row.id\">\r\n              <th scope=\"row\">{{ index + 1 }}</th>\r\n              <td>\r\n                {{ row.titulo }}\r\n              </td>\r\n              <td>\r\n                {{ row.lote }}\r\n              </td>\r\n              <td>\r\n                <router-link\r\n                  :to=\"{ name: 'autor_detalle', params: { id: row.id } }\"\r\n                  ><button class=\"btn btn-warning btn-sm mx-1\">\r\n                    Ver autor\r\n                  </button></router-link\r\n                >\r\n                <router-link\r\n                  :to=\"{ name: 'autor_edit', params: { id: row.id } }\"\r\n                >\r\n                  <button class=\"btn btn-primary btn-sm mx-1\">Editar</button>\r\n                </router-link>\r\n                <button\r\n                  @click=\"deleteAutor(row.id)\"\r\n                  class=\"btn btn-danger btn-sm\"\r\n                >\r\n                  Eliminar\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n  \r\n  <script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      libros: [],\r\n      autores: [],\r\n      libro: {\r\n        titulo: \"\",\r\n        lote: \"\",\r\n        descripcion: \"\",\r\n        autor_id: \"\",\r\n      },\r\n      errors: {},\r\n    };\r\n  },\r\n  created() {\r\n    this.getLibros();\r\n    this.getAutores();\r\n  },\r\n  methods: {\r\n    getLibros() {\r\n      this.axios\r\n        .get(\"http://127.0.0.1:8000/api/libro\")\r\n        .then((response) => {\r\n          console.log(response.data);\r\n          this.libros = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    getAutores() {\r\n      this.axios\r\n        .get(\"http://127.0.0.1:8000/api/autor\")\r\n        .then((response) => {\r\n          console.log(response.data);\r\n          this.autores = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    saveLibro() {\r\n      //alert('jola')\r\n      this.axios\r\n        .post(\"http://127.0.0.1:8000/api/autor-libro\", this.libro)\r\n        .then((response) => {\r\n          console.log(response.data);\r\n\r\n          this.autores.push(response.data);\r\n          this.autor = {\r\n            name: \"\",\r\n          };\r\n\r\n          this.errors = [];\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n          this.errors = Object.values(error.response.data.errors).flat();\r\n        });\r\n    },\r\n    deleteAutor(id) {\r\n      alert(id);\r\n      this.axios\r\n        .get(\"http://127.0.0.1:8000/api/autor-eliminar/\" + id)\r\n        .then(() => {\r\n          this.autores = this.autores.filter((autor) => autor.id != id);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n  \r\n  <style>\r\n</style>"],"mappings":";AAoHA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE;QACLC,MAAM,EAAE,EAAE;QACVC,IAAI,EA